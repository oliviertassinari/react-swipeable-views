{
  "name": "react-swipeable-views",
  "version": "0.6.2",
  "description": "A React component for swipeable views",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test docs",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "NODE_ENV=release babel-node test/unit.js",
    "build": "rm -rf lib && mkdir lib && NODE_ENV=release babel src --out-dir lib",
    "docs:browser:development": "cd docs && NODE_ENV=docs-browser-development babel-node webpack.js --dev",
    "docs:browser:production": "cd docs && NODE_ENV=docs-browser-production babel-node webpack.js",
    "docs:native:development": "NODE_ENV=docs-native-development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oliviertassinari/react-swipeable-views.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-native",
    "swipe",
    "swipeable",
    "slider"
  ],
  "author": "Olivier Tassinari <olivier.tassinari@gmail.com> (https://github.com/oliviertassinari)",
  "bugs": {
    "url": "https://github.com/oliviertassinari/react-swipeable-views/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^15.0.0 || ^0.14.0",
    "react-dom": "^15.0.0 || ^0.14.0",
    "react-motion": "^0.4.0"
  },
  "files": [
    "lib",
    "src"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-1": "^6.3.13",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.12.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.21.0",
    "ip": "^1.0.1",
    "jsdom": "^9.2.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.1",
    "material-ui": "^0.15.0",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
    "react-dom": "^0.14.0",
    "react-motion": "^0.4.4",
    "react-native": "^0.28.0",
    "react-native-button": "^1.5.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "recompose": "^0.20.0",
