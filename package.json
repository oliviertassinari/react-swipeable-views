{
  "name": "SwipeableViewsDocs",
  "private": true,
  "scripts": {
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "test": "npm run lint && npm run test:unit && npm run flow",
    "test:unit": "NODE_ENV=release mocha",
    "test:watch": "NODE_ENV=release mocha -w",
    "start": "npm run docs:browser:dev",
    "docs:browser:dev": "NODE_ENV=docs-browser-development babel-node ./node_modules/.bin/webpack-dev-server --config=docs/webpack/developmentConfig.js --progress",
    "docs:browser:build": "rm -rf docs/dist && NODE_ENV=docs-browser-production babel-node ./node_modules/.bin/webpack --config=docs/webpack/productionConfig.js --progress",
    "docs:native:dev": "NODE_ENV=docs-native-development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "flow": "flow",
    "prettier": "find . -name \"*.js\" | grep -v -f .eslintignore | xargs prettier --write --single-quote --trailing-comma all --print-width 100",
    "bootstrap": "lerna bootstrap && npm run build",
    "build": "rm -rf packages/*/lib && NODE_ENV=release lerna exec -- babel src --out-dir lib --ignore spec.js",
    "watch": "lerna exec --concurrency 99 -- babel src --out-dir lib --watch",
    "publish": "npm run build && lerna exec -- npm run prepublish && lerna publish"
  },
  "author": "Olivier Tassinari <olivier.tassinari@gmail.com> (https://github.com/oliviertassinari)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliviertassinari/react-swipeable-views.git"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native": "^2.1.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chai": "^4.1.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.2.1",
    "flow-bin": "^0.38.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.1.0",
    "lerna": "^2.0.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "material-ui": "^1.0.0-beta.2",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-native": "^0.42.3",
    "react-native-button": "^1.8.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "recompose": "^0.25.0",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
