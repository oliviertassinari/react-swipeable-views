{
  "privte": true,
  "scripts": {
    "lint": "eslint . && echo \"eslint: no lint errors\"",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "NODE_ENV=release babel-node test/unit.js",
    "test:unit:watch": "NODE_ENV=release babel-node test/watch.js",
    "docs:browser:development": "cd docs && NODE_ENV=docs-browser-development babel-node webpack.js --dev",
    "docs:browser:production": "cd docs && NODE_ENV=docs-browser-production babel-node webpack.js",
    "docs:native:development": "NODE_ENV=docs-native-development node node_modules/react-native/local-cli/cli.js start --reset-cache --verbose",
    "flow": "flow",
    "bootstrap": "lerna bootstrap && npm run build",
    "build": "rm -rf packages/*/lib && lerna exec -- babel src --out-dir lib --ignore spec.js",
    "watch": "lerna exec --concurrency 99 -- babel src --out-dir lib --watch",
    "prepublish": "npm run build && lerna exec -- pkgfiles"
  },
  "author": "Olivier Tassinari <olivier.tassinari@gmail.com> (https://github.com/oliviertassinari)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.3.13",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-runtime": "^6.20.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.35.0",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.24.1",
    "ip": "^1.1.4",
    "jsdom": "^9.8.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.1",
    "material-ui": "^0.16.5",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-motion": "^0.4.5",
    "react-native": "^0.39.2",
    "react-native-button": "^1.7.1",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.21.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.0",
    "unused-files-webpack-plugin": "^3.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
